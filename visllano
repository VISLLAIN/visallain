TextLabel.Parent = Villaino
TextLabel.AnchorPoint = Vector2.new(0.5, 0.5)
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.BackgroundTransparency = 1.000
TextLabel.Position = UDim2.new(0.475443155, 0, 0.818425894, 0)
TextLabel.Size = UDim2.new(0.370072961, 0, 0.049814757, 0)
TextLabel.Font = Enum.Font.Gotham
TextLabel.Text = "COLOCA EN LOS 2 CUBOS EL NUMERO DEL VASO DONDE APARECIO EL DIAMANTE"
