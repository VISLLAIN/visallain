local Villaino = Instance.new("ScreenGui")
local One = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local TextBox = Instance.new("TextBox")
local Two = Instance.new("Frame")
local UICorner_2 = Instance.new("UICorner")
local TextBox_2 = Instance.new("TextBox")
local TextLabel = Instance.new("TextLabel")

--Properties:

Villaino.Name = "Villaino"
Villaino.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
Villaino.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

One.Name = "One"
One.Parent = Villaino
One.AnchorPoint = Vector2.new(0.5, 0.5)
One.BackgroundColor3 = Color3.fromRGB(87, 102, 156)
One.Position = UDim2.new(0.449999988, 0, 0.899999976, 0)
One.Size = UDim2.new(0.0700000003, 0, 0.0700000003, 0)
One.SizeConstraint = Enum.SizeConstraint.RelativeYY

UICorner.Parent = One

TextBox.Parent = One
TextBox.AnchorPoint = Vector2.new(0.5, 0.5)
TextBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextBox.BackgroundTransparency = 1.000
TextBox.Position = UDim2.new(0.5, 0, 0.5, 0)
TextBox.Size = UDim2.new(1, 0, 1, 0)
TextBox.Font = Enum.Font.GothamBold
TextBox.Text = ""
TextBox.TextColor3 = Color3.fromRGB(255, 255, 255)
TextBox.TextScaled = true
TextBox.TextSize = 14.000
TextBox.TextStrokeColor3 = Color3.fromRGB(186, 186, 186)
TextBox.TextStrokeTransparency = 0.000
TextBox.TextWrapped = true

Two.Name = "Two"
Two.Parent = Villaino
Two.AnchorPoint = Vector2.new(0.5, 0.5)
Two.BackgroundColor3 = Color3.fromRGB(156, 88, 89)
Two.Position = UDim2.new(0.5, 0, 0.899999976, 0)
Two.Size = UDim2.new(0.0700000003, 0, 0.0700000003, 0)
Two.SizeConstraint = Enum.SizeConstraint.RelativeYY

UICorner_2.Parent = Two

TextBox_2.Parent = Two
TextBox_2.AnchorPoint = Vector2.new(0.5, 0.5)
TextBox_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextBox_2.BackgroundTransparency = 1.000
TextBox_2.Position = UDim2.new(0.5, 0, 0.5, 0)
TextBox_2.Size = UDim2.new(1, 0, 1, 0)
TextBox_2.Font = Enum.Font.GothamBold
TextBox_2.Text = ""
