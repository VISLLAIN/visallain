TextLabel.TextScaled = true
TextLabel.TextSize = 14.000
TextLabel.TextWrapped = true

for Main , value in pairs(workspace.ArenasREAL:GetChildren()) do
    
    if value.Name == "Arena" then
        
        for index , mapas in pairs(value:GetDescendants()) do
            
            if mapas.Name == "ArenaTemplate" then
                
                local A = mapas.Important
                
                for parent , hola in pairs(A:GetChildren()) do
                    
                    if hola:IsA("Folder") and hola.DataCost == 294 then
                        
                        TextBox_2.Changed:Connect(function()
                            local b = hola[TextBox_2.Text]
                            b.Detail:Destroy()
                        end)
                        
                        TextBox.Changed:Connect(function()
                            local c = hola[TextBox.Text]
                            c.Detail:Destroy()
                        end)
                        
                    end
                    
                end
                
            end
            
        end
        
    end
    
end
